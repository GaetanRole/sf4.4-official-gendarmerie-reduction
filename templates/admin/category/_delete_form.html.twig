<form method="post" action="{{ path('app_admin_category_delete', {'uuid': category.uuid}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
    <button class="btn">{{ 'btn.delete'|trans }}</button>
</form>
