{# templates/base.html.twig #}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="Gaëtan Rolé-Dubruille">
    <meta name="description" content="{% block description %}Offres et Réductions Gendarmerie est un site centralisant toutes les offres et réductions de différents commerces pour le personnel de la Gendarmerie Nationale.{% endblock %}">
    <meta name="keywords" content="">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

    <title>Gendarmerie - {% block title %}Offres et Réductions{% endblock %}</title>

    {% block stylesheets %}
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('assets/images/favicon.ico') }}">
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('assets/images/favicon.ico') }}">
    <link rel="apple-touch-icon" type="image/x-icon" href="{{ asset('assets/images/favicon.ico') }}">
</head>
<body>
<header>
    {% block header %}
    {% endblock %}
</header>
<div id="wrapper">
    {% block body %}{% endblock %}
</div>
<footer>
    {% block footer %}
    {% endblock %}
</footer>

{% block javascripts %}
{% endblock %}
</body>
</html>